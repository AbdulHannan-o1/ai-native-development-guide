{
  "version": 2,
  "installCommand": "pip install -r book-source/api/requirements.txt && cd book-source && pnpm install",
  "buildCommand": "cd book-source && pnpm build",
  "functions": {
    "book-source/api/src/main.py": {
      "runtime": "python3.10",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "book-source/api/src/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "build/$1"
    }
  ],
  "outputDirectory": "book-source/build"
}